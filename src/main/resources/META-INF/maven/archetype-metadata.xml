<archetype-descriptor
        name="gRPCtoJakartaREST-archetype">
        
   <requiredProperties>
      <requiredProperty key="generate-prefix"/>
      <requiredProperty key="generate-package"/>
      <requiredProperty key="resteasy-version"/>
      <requiredProperty key="grpc-bridge-version"/>
   </requiredProperties>
  
   <fileSets>
       <fileSet filtered="true">
          <directory>src/main/proto/</directory>
       </fileSet>
       <fileSet filtered="true">
          <directory>src/main/resources</directory>
       </fileSet>
       <fileSet filtered="true">
          <directory>src/main/webapp/</directory>
       </fileSet>
       <fileSet filtered="true">
          <directory>src/main/java/</directory>
       </fileSet>
   </fileSets>
</archetype-descriptor>